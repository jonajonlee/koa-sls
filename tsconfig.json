{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": ["es2016", "ScriptHost", "es2015.promise"],
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "./src",
    "strict": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "typeRoots": ["./typings"],
    "types": ["node"]
  },
  "exclude": ["node_modules"]
}
